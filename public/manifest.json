{
  "short_name": "Note'd ",
  "name": "Note'd",
  "description": "Stores GFM Flavored Markdown Notes",
  "icons": [
    {
      "src": "/noted-logo-animated.gif",
      "sizes": "32x32",
      "type": "image/gif"
    }
  ],
  "app": {
    "urls": [
      "https://note-d.app"
    ],
    "launch": {
      "web_url": "https://note-d.app"
    }
  },
  "share_target": {
    "action": "/create-note/",
    "method": "POST",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display": "fullscreen",
  "start_url": "/",
  "orientation": "any",
  "theme_color": "#2C323B",
  "background_color": "#588c89"
}
